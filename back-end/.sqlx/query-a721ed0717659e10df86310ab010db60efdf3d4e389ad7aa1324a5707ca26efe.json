  "query": "\n            SELECT \n                fp.id, fp.user_id, fp.content, fp.like_count, fp.comment_count,\n                fp.created_at, fp.updated_at,\n                u.full_name\n            FROM feed_posts fp\n            JOIN users u ON fp.user_id = u.id\n            WHERE fp.id = $1\n            ",
